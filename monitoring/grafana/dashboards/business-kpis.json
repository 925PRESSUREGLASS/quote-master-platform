{
  "dashboard": {
    "id": null,
    "title": "Quote Master Pro - Business KPIs",
    "tags": ["quote-master", "business", "kpi"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Daily Active Users",
        "type": "stat",
        "targets": [
          {
            "expr": "active_users_current",
            "legendFormat": "Active Users"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Quotes Generated Today",
        "type": "stat",
        "targets": [
          {
            "expr": "increase(quotes_generated_total[24h])",
            "legendFormat": "Quotes Today"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Revenue (Hourly)",
        "type": "stat",
        "targets": [
          {
            "expr": "revenue_current_hour",
            "legendFormat": "Revenue/Hour"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Customer Satisfaction",
        "type": "gauge",
        "targets": [
          {
            "expr": "customer_satisfaction_score",
            "legendFormat": "Satisfaction Score"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 1,
            "max": 5,
            "thresholds": {
              "steps": [
                {"color": "red", "value": 1},
                {"color": "yellow", "value": 3},
                {"color": "green", "value": 4}
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 0
        }
      },
      {
        "id": 5,
        "title": "Quote Generation Trends",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(quotes_generated_total[1h])",
            "legendFormat": "Quotes/Hour"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 6,
        "title": "User Session Duration",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(user_session_duration_seconds_bucket[5m]))",
            "legendFormat": "95th Percentile"
          },
          {
            "expr": "histogram_quantile(0.50, rate(user_session_duration_seconds_bucket[5m]))",
            "legendFormat": "Median"
          }
        ],
        "yAxes": [
          {
            "unit": "s"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 7,
        "title": "Top Performing Endpoints",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, sum by (endpoint) (rate(api_requests_by_endpoint_total[1h])))",
            "legendFormat": "{{endpoint}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 8,
        "title": "Error Rate by Endpoint",
        "type": "graph",
        "targets": [
          {
            "expr": "sum by (endpoint) (rate(http_requests_total{status_code=~\"5.*\"}[5m])) / sum by (endpoint) (rate(http_requests_total[5m]))",
            "legendFormat": "{{endpoint}}"
          }
        ],
        "yAxes": [
          {
            "unit": "percentunit",
            "max": 0.1
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      }
    ],
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "refresh": "30s"
  }
}
